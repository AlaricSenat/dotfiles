BinPackArguments: 'false'
BinPackParameters: 'false'
BreakBeforeBraces: Allman
ColumnLimit: '80'
IndentWidth: '4'

IncludeBlocks: 'Regroup'
IncludeCategories:
    - Regex:           'config\.h'
      Priority:        -2
    - Regex:           'vlc_common\.h'
      Priority:        2
    - Regex:           '^<vlc_.*'
      Priority:        3
    - Regex:           '^<.*'
      Priority:        1
    - Regex:           '^".*'
      Priority:        4
