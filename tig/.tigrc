bind main <Ctrl-F> !git commit --fixup=%(commit)
bind diff <Ctrl-F> !git commit --fixup=%(commit)
bind main <Ctrl-R> !git-revise --autosquash -i %(commit)^
bind diff <Ctrl-R> !git-revise --autosquash -i %(commit)^
bind main <Ctrl-E> !git rebase --autosquash -i %(commit)^
bind diff <Ctrl-E> !git rebase --autosquash -i %(commit)^
